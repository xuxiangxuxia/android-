<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:orientation="vertical">
   <EditText
       android:layout_width="match_parent"
       android:layout_height="200dp"
       android:id="@+id/editText_1"
       android:hint="请输入"
       android:textSize="50dp"
       />
    <LinearLayout
        android:id="@+id/liner_1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:layout_marginTop="20dp"
        android:background="#F7F6F6">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:id="@+id/liner_2"
            android:orientation="horizontal"
            android:layout_weight="1">
            <Button
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/button_clear"
                android:text="C"
                android:layout_weight="1"
                android:layout_marginRight="5dp"
                android:background="@drawable/bg_c"
                android:textSize="35sp"
                android:textColor="#1296db"
            />

            <ImageButton
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/button_div"
                android:layout_weight="1"
                android:layout_marginRight="5dp"
                android:src="@mipmap/div"
                android:scaleType="fitXY"
                android:paddingBottom="28dp"
                android:paddingEnd="32dp"
                android:paddingLeft="32dp"
                android:paddingTop="28dp"
                android:background="@drawable/bg_c"
                />
            <ImageButton
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/button_mul"
                android:text="*"
                android:layout_weight="1"
                android:layout_marginRight="5dp"
                android:src="@mipmap/mul"
                android:scaleType="fitXY"
                android:paddingBottom="32dp"
                android:paddingEnd="35dp"
                android:paddingLeft="35dp"
                android:paddingTop="32dp"
                android:background="@drawable/bg_c"
                />
            <ImageButton
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/button_del"
                android:layout_weight="1"
                android:src="@mipmap/delete"
                android:scaleType="fitXY"
                android:paddingBottom="32dp"
                android:paddingEnd="35dp"
                android:paddingLeft="35dp"
                android:paddingTop="32dp"
                android:background="@drawable/bg_c"

                />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:id="@+id/liner_3"
            android:orientation="horizontal"
            android:layout_weight="1">
            <ImageButton
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/button_7"
                android:layout_weight="1"
                android:layout_marginRight="5dp"
                android:src="@mipmap/seven"
                android:scaleType="fitXY"
                android:paddingBottom="18dp"
                android:paddingEnd="18dp"
                android:paddingLeft="18dp"
                android:paddingTop="18dp"
                android:background="@drawable/bg_c"
                />
            <ImageButton
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/button_8"
                android:layout_weight="1"
                android:layout_marginRight="5dp"
                android:src="@mipmap/eight"
                android:scaleType="fitXY"
                android:paddingBottom="18dp"
                android:paddingEnd="18dp"
                android:paddingLeft="18dp"
                android:paddingTop="18dp"
                android:background="@drawable/bg_c"
                />
            <ImageButton
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/button_9"
                android:layout_weight="1"
                android:layout_marginRight="5dp"
                android:src="@mipmap/nine"
                android:scaleType="fitXY"
                android:paddingBottom="18dp"
                android:paddingEnd="18dp"
                android:paddingLeft="18dp"
                android:paddingTop="18dp"
                android:background="@drawable/bg_c"
                />
            <ImageButton
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/button_sub"
                android:layout_weight="1"
                android:src="@mipmap/sub"
                android:scaleType="fitXY"
                android:paddingBottom="20dp"
                android:paddingEnd="35dp"
                android:paddingLeft="35dp"
                android:paddingTop="20dp"
                android:background="@drawable/bg_c"

                />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:id="@+id/liner_4"
            android:orientation="horizontal"
            android:layout_weight="1">
            <ImageButton
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/button_4"
                android:layout_weight="1"
                android:layout_marginRight="5dp"
                android:src="@mipmap/four"
                android:scaleType="fitXY"
                android:paddingBottom="18dp"
                android:paddingEnd="18dp"
                android:paddingLeft="18dp"
                android:paddingTop="18dp"
                android:background="@drawable/bg_c"
                />
            <ImageButton
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/button_5"
                android:layout_weight="1"
                android:layout_marginRight="5dp"
                android:src="@mipmap/five"
                android:scaleType="fitXY"
                android:paddingBottom="18dp"
                android:paddingEnd="18dp"
                android:paddingLeft="18dp"
                android:paddingTop="18dp"
                android:background="@drawable/bg_c"
                />
            <ImageButton
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/button_6"
                android:layout_weight="1"
                android:layout_marginRight="5dp"
                android:src="@mipmap/six"
                android:scaleType="fitXY"
                android:paddingBottom="18dp"
                android:paddingEnd="18dp"
                android:paddingLeft="18dp"
                android:paddingTop="18dp"
                android:background="@drawable/bg_c"
                />
            <ImageButton
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/button_add"
                android:layout_weight="1"
                android:src="@mipmap/add"
                android:scaleType="fitXY"
                android:paddingBottom="26dp"
                android:paddingEnd="32dp"
                android:paddingLeft="32dp"
                android:paddingTop="26dp"
                android:background="@drawable/bg_c"
                />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/liner_5"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="2"
            android:orientation="horizontal"
            >
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/rela_1">

                <Button
                    android:id="@+id/button_equal"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_alignParentRight="true"
                    android:layout_marginStart="5dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginEnd="5dp"
                    android:layout_marginBottom="5dp"
                    android:background="@drawable/bg_1"
                    android:text="="
                    android:textColor="@color/white"
                    android:textSize="55sp" />
               <LinearLayout
                   android:layout_width="match_parent"
                   android:layout_height="match_parent"
                   android:id="@+id/liner_6"
                   android:orientation="vertical"
                   android:layout_toLeftOf="@+id/button_equal" >
                   <LinearLayout
                       android:layout_width="match_parent"
                       android:layout_height="match_parent"
                       android:id="@+id/liner_7"
                       android:layout_weight="1"
                       android:orientation="horizontal">
                       <ImageButton  android:layout_width="match_parent"
                           android:layout_height="match_parent"
                           android:id="@+id/button_1"
                           android:layout_weight="1"
                           android:layout_marginRight="5dp"
                           android:src="@mipmap/one"
                           android:scaleType="fitXY"
                           android:paddingBottom="18dp"
                           android:paddingEnd="18dp"
                           android:paddingLeft="18dp"
                           android:paddingTop="18dp"
                           android:background="@drawable/bg_c"

                       />
                       <ImageButton  android:layout_width="match_parent"
                           android:layout_height="match_parent"
                           android:id="@+id/button_2"
                           android:layout_weight="1"
                           android:layout_marginRight="5dp"
                           android:src="@mipmap/two"
                           android:scaleType="fitXY"
                           android:paddingBottom="18dp"
                           android:paddingEnd="18dp"
                           android:paddingLeft="18dp"
                           android:paddingTop="18dp"
                           android:background="@drawable/bg_c"
                           />

                       <ImageButton
                           android:id="@+id/button_3"
                           android:layout_width="match_parent"
                           android:layout_height="match_parent"
                           android:layout_marginRight="5dp"
                           android:layout_weight="1"
                           android:src="@mipmap/three"
                           android:scaleType="fitXY"
                           android:paddingBottom="18dp"
                           android:paddingEnd="18dp"
                           android:paddingLeft="18dp"
                           android:paddingTop="18dp"
                           android:background="@drawable/bg_c"/>
                   </LinearLayout>
                   <LinearLayout
                       android:layout_width="match_parent"
                       android:layout_height="match_parent"
                       android:id="@+id/liner_8"
                       android:layout_weight="1"
                       android:orientation="horizontal">
                       <ImageButton  android:layout_width="match_parent"
                           android:layout_height="match_parent"
                           android:id="@+id/button_sqrt"
                           android:layout_weight="1"
                           android:layout_marginRight="5dp"
                           android:src="@mipmap/square"
                           android:scaleType="fitXY"
                           android:paddingBottom="30dp"
                           android:paddingEnd="30dp"
                           android:paddingLeft="30dp"
                           android:paddingTop="30dp"
                           android:background="@drawable/bg_c"
                           />
                       <ImageButton  android:layout_width="match_parent"
                           android:layout_height="match_parent"
                           android:id="@+id/button_0"
                           android:layout_weight="1"
                           android:layout_marginRight="5dp"
                           android:src="@mipmap/zero"
                           android:scaleType="fitXY"
                           android:paddingBottom="18dp"
                           android:paddingEnd="18dp"
                           android:paddingLeft="18dp"
                           android:paddingTop="18dp"
                           android:background="@drawable/bg_c"
                           />
                       <ImageButton  android:layout_width="match_parent"
                           android:layout_height="match_parent"
                           android:id="@+id/button_point"
                           android:layout_weight="1"
                           android:layout_marginRight="5dp"
                           android:src="@mipmap/point"
                           android:scaleType="fitXY"
                           android:paddingBottom="32dp"
                           android:paddingEnd="35dp"
                           android:paddingLeft="35dp"
                           android:paddingTop="32dp"
                           android:background="@drawable/bg_c"
                           />
                   </LinearLayout>


               </LinearLayout>


            </RelativeLayout>
        </LinearLayout>

    </LinearLayout>
</LinearLayout>
